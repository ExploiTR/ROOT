<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="app.exploitr.root.axes"
    android:versionCode="1"
    android:versionName="Beta" android:installLocation="internalOnly"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.ACCESS_SUPERUSER"/>
    
    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="19" tools:ignore="OldTargetApi"/>
    
    <application
        android:allowBackup="false"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.Holo.Light.DarkActionBar" >
        <activity
            android:name="app.exploitr.root.axes.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:launchMode="singleInstance" android:theme="@android:style/Theme.Holo.Light.DarkActionBar" android:name="app.exploitr.root.axes.InfoActivity" android:label="@string/app_name" >
    </activity>
    </application>

</manifest>
